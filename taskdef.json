{
    "executionRoleArn": "arn:aws:iam::469808146940:role/python-library-role",
    "containerDefinitions": [
        {
            "name": "python-library",
            "image": "469808146940.dkr.ecr.us-west-2.amazonaws.com/data-science-helpers:latest",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 8000,
                    "protocol": "tcp",
                    "containerPort": 8000
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "python-library"
}